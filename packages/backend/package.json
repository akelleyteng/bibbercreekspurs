{
  "name": "@4hclub/backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "migrate": "node -r ts-node/register src/scripts/migrate.ts",
    "seed": "node -r ts-node/register src/scripts/seed.ts",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@google-cloud/storage": "^7.0.0",
    "bcrypt": "^6.0.0",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dataloader": "^2.2.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.0.0",
    "glob": "^13.0.3",
    "googleapis": "^126.0.0",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^8.0.1",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.2.0",
    "type-graphql": "2.0.0-rc.2",
    "vite": "7.3.1",
    "winston": "^3.10.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^2.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0"
  }
}
